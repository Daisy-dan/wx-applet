/* pages/account/account.wxss */
.text-center{
    text-align: center;
}
.text-center text{
    color: #666666;
    font-size: 16px;
    vertical-align: middle;
}
.padd{
    padding: 30px 0;
    border-bottom: 1px solid #e4e4e4;
    background: #ffffff;
    color: #000000;
    font-size: 26px;
    font-weight: bold;
    font-family: Arial, Helvetica, sans-serif;
}
.title{
    color: #ffffff;
    font-size: 16px;
    line-height: 34px;
    padding-left: 20px;
    background: #f05139;
    border-bottom: 1px solid #e4e4e4;
}
.p20{
    padding:0 20px; 
    background: #ffffff;
}
.list{
    padding:10px 0;
    border-bottom: 1px solid #e4e4e4;
    font-size: 16px;
    font-family: Arial, Helvetica, sans-serif;
}
.list image{
    width: 50px;
    height:50px;
    border-radius: 50%;
    border: 1px solid #e4e4e4;
    vertical-align: middle;
    margin-right: 20px;
}
.name{
   margin-right: 10px;
}
.reward{
    color: #f05139;
}
.time{
    color: #999999;
    font-size: 12px;
    margin-left: 70px;
    margin-top: -10px;
}