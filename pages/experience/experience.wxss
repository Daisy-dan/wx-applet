/* pages/experience/experience.wxss */
.focus{
    background: #ffffff;
    line-height: 50px;
    text-align: center;
}
.focus text{
    color: #999999;
    font-size: 15px;
    margin: 0 5px;
    vertical-align: middle;
}
.reward{
    background: #333333;
    line-height: 50px;
    text-align: center;
    color: #ffffff;
    font-size: 16px;
    font-family: Arial, Helvetica, sans-serif;
}
.reward text{
    color: #f05139;
    font-weight: bold;
    font-size: 20px;
    
}
.code{
    padding:50px 20px;
    text-align: center;
    background: #ffffff;
}
.codeView{
    height: 230px;
    border: 1px solid #e4e4e4;
    text-align: center;
}
.codeView text{
    line-height: 230px;
    font-size: 16px;
    color: #999999;
}
.tips{
    line-height: 50px;
    font-size: 16px;
    color: #333333;
}
